    <tags>TDD BDD fluent netcore netstandard xUnit2</tags>
    <releaseNotes></releaseNotes>
    <dependencies>
      <group>
        <dependency id="ImpruvIT.DataValidation" version="0.1.0" />
        <dependency id="xunit.assert" version="2.3.1" />
      </group>
      <group targetFramework=".NETStandard2.0" />
    </dependencies>
  </metadata>
  <files>
    <file src="netstandard1.1\ImpruvIT.DataValidation.dll" target="lib\netstandard1.1" />
